<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="extension-development-utility">
  <schema path="/org/gnome/shell/extensions/extension-development-utility/" id="org.gnome.shell.extensions.extension-development-utility">
    <key type="i" name="panel-icon-position">
      <default>0</default>
      <summary>0, 1, 2 or 3.</summary>
      <description>Panel button icon position : 0 =  left center, 1 = right center, 2 = right, 3 = far-right.</description>
    </key>
    <key type="b" name="submenu-is-open">
      <default>true</default>
      <summary>Open if true.</summary>
      <description>Extensions submenu open, or not.</description>
    </key>
  </schema>
</schemalist>
